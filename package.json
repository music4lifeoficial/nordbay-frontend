{
  "name": "nordbay-frontend",
  "version": "1.0.0",
  "description": "Nordic Marketplace for Conscious Consumers - Frontend (Vercel + Railway)",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/music4lifeoficial/nordbay-frontend"
  },
  "author": "NordBay Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "ANALYZE=true npm run build",
    "vercel-build": "npm run build",
    "prepare": "husky install"
